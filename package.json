{
    "name": "beispiel",
    "version": "1.0.0",
    "description": "Beispiel",
    "main": "dist/server/index.js",
    "scripts": {
        "prestart": "ts-node scripts/init",
        "start": "nodemon --verbose",
        "pretest": "ts-node scripts/init && npm run mongo import && if exist server.log del server.log",
        "test": "npx mocha dist\\test\\**\\*.spec.js",
        "clean": "ts-node scripts/clean",
        "debug": "nodemon --inspect dist/server/index.js",
        "eslint": "npx eslint src/**/*.ts scripts/*.ts",
        "fixme": "ts-node scripts/fixme",
        "preh2": "ts-node scripts/init",
        "h2": "node --expose-http2 dist/server/index.js",
        "mailserver": "java -jar C:/Zimmermann/fakesmtp/fakeSMTP-2.0.jar --start-server --port 25000 --memory-mode",
        "mongo": "ts-node scripts/mongo",
        "prettier": "npx prettier --write src/**/*.ts",
        "preserve": "ts-node scripts/init",
        "serve": "node dist/server/index.js",
        "pretsc": "npm run prettier",
        "tsc": "npx --no-install tsc -w"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm t",
            "pre-push": "npm t"
        }
    },
    "private": true,
    "homepage": "https://www.hs-karlsruhe.de",
    "pre-commit": [
        "npm run eslint"
    ],
    "keywords": [
        "Node",
